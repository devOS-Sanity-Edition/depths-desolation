{
	"schema_version": 1,
	"quilt_loader": {
		"group": "one.devos.nautical",
		"id": "modid",
		"version": "${version}",
		"metadata": {
			"name": "Modid",
			"description": "A short description of your mod.",
      "contributors": {
        "devOS: Sanity Edition, Team Nautical": "Owner"
      },
      "contact": {
        "issues": "https://github.com/devOS-Sanity-Edition/working-title/issues",
        "sources": "https://github.com/devOS-Sanity-Edition/working-title"
      },
			"icon": "assets/modid/icon.png"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
		"entrypoints": {
			"init": "one.devos.nautical.modid.ExampleMod"
		},
		"depends": [
			{
				"id": "quilt_loader",
				"versions": ">=${loader_version}"
			},
			{
				"id": "quilted_fabric_api",
				"versions": ">=${qfapi_version}"
			},
			{
				"id": "minecraft",
				"versions": ">=${minecraft_version}"
			}
		]
	},
	"mixin": "modid.mixins.json"
}
